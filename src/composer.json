{
    "name": "slim/slim-challenge",
    "description": "PHP Challenge - slim",
    "type": "project",
    "require": {
        "php": "^7.3 || ^8.0",
        "slim/slim": "^4.7",
        "slim/psr7": "^1.3",
        "swiftmailer/swiftmailer": "^6.0",
        "php-di/php-di": "^6.3",
        "symfony/dotenv": "^5.2",
        "tuupola/slim-basic-auth": "^3.3",
        "php-amqplib/php-amqplib": "^3.2",
        "ext-json": "*",
        "tuupola/slim-jwt-auth": "^3.6",
        "illuminate/support": "^7.0.0",
        "illuminate/database": "^7.4",
        "robmorgan/phinx": "^0.12.10",
        "guzzlehttp/guzzle": "^7.0"
    },
    "scripts": {
        "start": "php -S localhost:8080 -t public",
        "test": "phpunit"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "files": [
            "./app/helpers.php"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "kylekatarnls/update-helper": true
        }
    }
}
